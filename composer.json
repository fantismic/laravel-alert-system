{
  "name": "fantismic/alert-system",
  "description": "Reusable Laravel alerting system",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Fantismic\\AlertSystem\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Fantismic\\AlertSystem\\AlertSystemServiceProvider"
      ],
      "aliases": {
        "Alert": "Fantismic\\AlertSystem\\Facades\\Alert"
      }
    }
  },
  "require": {
    "php": "^8.1",
    "illuminate/support": "^11.0"
  }
}
